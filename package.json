    "storybook:build": "build-storybook -c configs/.storybook -s public",
    "post-update": "echo \"codesandbox preview only, need an update\" && yarn upgrade --latest",
    "prepare": "husky install configs/husky",
    "chromatic": "npx chromatic --project-token=666f1a8da5d3"
  },
  "stylelint": {
    "extends": "./configs/stylelint/stylelint.config.js"
  },
