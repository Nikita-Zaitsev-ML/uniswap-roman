    "dev": "open-cli http://localhost:3000 && next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest --config=configs/jest/jest.config.js --watch",
    "lint": "stylelint ./src/**/*.style.ts && eslint ./src/ --ext .ts,.tsx && prettier ./src/ --ignore-unknown --check",
